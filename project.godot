[application]

config/name="Exodus"
run/main_scene="res://Menu/Menus/MenuController.tscn"
boot_splash/show_image=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Transition="*res://AutoLoad/transition.tscn"
BgmPlayer="*res://AutoLoad/BGM_player.tscn"
ButtonClickSfx="*res://AutoLoad/button_click_sfx.tscn"
PluginSettings="*res://addons/github-integration/scripts/PluginSettings.gd"
IconLoaderGithub="*res://addons/github-integration/scripts/IconLoaderGithub.gd"
UserData="*res://addons/github-integration/scripts/user_data.gd"
RestHandler="*res://addons/github-integration/scenes/RestHandler.tscn"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/github-integration/plugin.cfg" )

[global]

font=false
display=false
boot=false
vsy=false

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom_font="res://Assets/font/MainGameFont.tres"

