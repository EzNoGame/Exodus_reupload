[application]

config/name="Exodus"
run/main_scene="res://Menu/Intro.tscn"
boot_splash/show_image=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

Transition="*res://AutoLoad/transition.tscn"
BgmPlayer="*res://AutoLoad/BGM_player.tscn"
ButtonClickSfx="*res://AutoLoad/button_click_sfx.tscn"
Spawner="*res://AutoLoad/Spawner.gd"
DataLoader="*res://AutoLoad/DataLoader.tscn"
SfxController="*res://AutoLoad/SFX_Controller.tscn"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-sqlite/plugin.cfg" )

[global]

font=false
display=false
boot=false
vsy=false
start=false

[gui]

